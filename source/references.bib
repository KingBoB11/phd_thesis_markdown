@article{logging,
author = {Zeng, Lei and Xiao, Yang and Chen, Hui and Sun, Bo and Han, Wenlin},
title = {Computer operating system logging and security issues: a survey},
journal = {Security and Communication Networks},
volume = {9},
number = {17},
pages = {4804-4821},
keywords = {logging, operating system, Linux, Unix, security},
doi = {https://doi.org/10.1002/sec.1677},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/sec.1677},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/sec.1677},
abstract = {Abstract Logging has become a fundamental feature within the modern computer operating systems because of the fact that logging may be used through a variety of applications and fashion, such as system tuning, auditing, and intrusion detection systems. Syslog daemon is the logging implementation in Unix/Linux platforms, while Windows Event Log is the logging implementation in Microsoft Windows platforms. These logging implementations provide application program interfaces that, in turn, simplify logging functions from data collection to data storage. In this paper, we survey Unix, Linux, and Windows logging mechanisms and introduce their security issues. Copyright © 2016 John Wiley \& Sons, Ltd.},
year = {2016}
}
